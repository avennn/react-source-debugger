{
  "name": "react-source-debugger",
  "version": "1.0.0",
  "description": "A tool for quickly setting up the environment of debugging React source code.",
  "main": "src/index.js",
  "type": "module",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "bin": {
    "rsd": "src/index.js"
  },
  "exports": {
    "node": {
      "import": "./src/index.js",
      "require": "./src/index.js"
    }
  },
  "scripts": {
    "postpublish": "node scripts/postPublish.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/avennn/react-source-debugger.git"
  },
  "keywords": [
    "react",
    "debugger",
    "debug",
    "source",
    "code",
    "sourcemap"
  ],
  "author": {
    "name": "Javen Leung",
    "email": "914301050@qq.com",
    "url": "https://github.com/avennn"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/avennn/react-source-debugger/issues"
  },
  "homepage": "https://github.com/avennn/react-source-debugger#readme",
  "dependencies": {
    "chalk": "^5.2.0",
    "commander": "^10.0.1",
    "deepmerge": "^4.3.1",
    "detect-port": "^1.5.1",
    "shelljs": "^0.8.5",
    "tori": "^2.0.0"
  }
}
